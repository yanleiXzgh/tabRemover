{
  "manifest_version": 3,
  "name": "标签页名修改",
  "description": "修改当前标签页的名称",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content-scripts/content-script.js"]
  }],
  "permissions": ["activeTab", "scripting", "tabs"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}